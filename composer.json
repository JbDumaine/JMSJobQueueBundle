{
    "name":         "jbdumaine/job-queue-bundle",
    "description":  "Support for PHP 8, Symfony 7 and Doctrine Common 3",
    "keywords":     ["job","queue", "background", "cron", "scheduled"],
    "homepage":     "http://jmsyst.com/bundles/JMSJobQueueBundle",
    "type":         "symfony-bundle",
    "license":      "Apache-2.0",
    "replace": {
        "jms/job-queue-bundle": "*"
    },
    "authors": [
        {
            "name":   "Jean Baptiste DUMIANE",
            "email":  "jbdumaine@ecofinance.fr"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/error-handler": "^7.0",
        "doctrine/common": "^3.0",
        "symfony/process": "^7.0",
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/class-loader": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "doctrine/orm": "*",
        "symfony/twig-bundle": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "phpunit/phpunit": "^9.4",
        "symfony/intl": "*"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Required when using the webinterface.",
        "symfony/twig-bundle": "Required when using the webinterface."
    },
    "autoload": {
        "psr-0": { "JMS\\JobQueueBundle": "" }
    },
    "target-dir": "JMS/JobQueueBundle"
}
